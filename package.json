{
  "name": "typescripttemplate",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Stanford BDHG TypeScript Template",
  "keywords": [
    "Stanford",
    "Biodesign"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/StanfordBDHG/TypeScriptTemplate"
  },
  "private": true,
  "scripts": {
    "postinstall": "npm --prefix packages/example-package run build",
    "dev": "concurrently \"next dev\" \"npm --prefix packages/example-package run watch\"",
    "build": "npm --prefix packages/example-package run build & next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix & prettier --write .",
    "lint:ci": "eslint --output-file eslint_report.json --format json .",
    "test": "jest"
  },
  "dependencies": {
    "@stanfordbdhg/example-package": "file:../packages/example-package",
    "next": "^14",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.32"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14",
    "@types/jest": "^29.5.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "concurrently": "^8",
    "eslint": "^8",
    "eslint-config-next": "^14",
    "eslint-config-prettier": "^9",
    "eslint-plugin-prettier": "^5",
    "jest": "^29",
    "jest-environment-jsdom": "^29",
    "postcss": "^8",
    "prettier": "^3",
    "ts-jest": "^29",
    "typescript": "^5"
  }
}
